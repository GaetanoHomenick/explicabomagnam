  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "onchange": "^3.2.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
